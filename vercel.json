{
    "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "redirects": [
    {
      "source": "https://spedire-sicuro-platform-cp1g.vercel.app/:path*",
      "destination": "https://spedire-sicuro.vercel.app/:path*",
      "permanent": true
    },
    {
      "source": "https://spedire-sicuro-platform.vercel.app/:path*",
      "destination": "https://spedire-sicuro.vercel.app/:path*",
      "permanent": true
    }
  ]
}
